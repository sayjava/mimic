{
    "id": "{{data.random.alphaNumeric 10}}",
    "availableForSale": false,
    "title": "{{data.commerce.productName}}",
    "productType": "{{data.random.word}}",
    "createdAt": "{{data.date.recent}}",
    "handle": "product-{{@index}}",
    "vendor": "{{data.company.name}}",
    "description": "{{data.lorem.sentences 4 '\n'}}",
    "descriptionHtml": "{{data.lorem.sentences 4 '\n'}}",
    "isGiftCard": true,
    "tags": [
    {{#repeat 3}}
        "{{data.random.word}}"   
    {{/repeat}}
    ],
    "options": [
        {
            "id": "{{data.random.alphaNumeric 10}}",
            "name": "{{data.random.word}}",
            "values": [
            {{#repeat 3}}
                "{{data.random.word}}"
            {{/repeat}}
            ]
        }
    ],
    "priceRange": {
        "maxVariantPrice": {{> money}},
        "minVariantPrice": {{> money}}
    },
    "featuredImage": {
        "url": "{{data.image.dataUrl 1200 1600}}",
        "altText": null,
        "width": 1200,
        "height": 1600
    },
    "images": {
        "pageInfo": {
            "hasNextPage": true,
            "hasPreviousPage": false
        },
         "edges": [
         {{#repeat 5}}
            {
                "node": {{> image}}
            }
          {{/repeat}}
        ]
    },
    "variants": {
        "pageInfo": {
            "hasNextPage": true,
            "hasPreviousPage": false
        },
        "edges": [
            {{#repeat 2}}
                {
                    "node": {{> product-variant}}
                }
            {{/repeat}}
        ]
    }
}
