  {
    "name": "Error Todo",
    "request": {
      "path": "/errors",
      "method": "POST|GET"
    },
    "response": {
      "status": 500,
      "headers": {
        "Content-Type": "application/json"
      },
      "body": "Errors"
    }
  },
  {
    "name": "Redirect",
    "request": {
      "path": "/redirect",
      "method": "POST|GET"
    },
    "response": {
      "status": 302,
      "headers": {
        "Content-Type": "application/json"
      },
      "body": "Redirect"
    }
  },
  {
    "name": "Not Found",
    "request": {
      "path": "/not-found",
      "method": "POST|GET"
    },
    "response": {
      "status": 404,
      "headers": {
        "Content-Type": "text/plain"
      },
      "body": "Not Found"
    }
  },
  {
    "name": "Forbidden",
    "request": {
      "path": "/forbidden",
      "method": "POST|GET"
    },
    "response": {
      "status": 403,
      "headers": {
        "Content-Type": "text/plain"
      },
      "body": "Forbidden"
    }
  }
